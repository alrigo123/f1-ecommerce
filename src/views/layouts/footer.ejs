<!--Modal: Login / Register Form-->
<div id="id01" class="modal">

	<form class="modal-content animate" action="/login" method="post">

		<div class="container" style="text-align: center;">
			<div class="row">
				<div class="col-sm-12">
					<div class="row text-center justify-content-center align-items-center" style="padding: 15px;">
						<div class="col-md-12 mb-2">
							<h3 style="color:#ffffff">LOGIN</h3>
						</div>
						<div class="col-md-11 text-left my-2">
							<i class="fas fa-user mr-1"></i> <label for="uname"><b>Your Username</b></label>
							<input type="text" placeholder="Enter Username" name="username" id="username" required>
						</div>
						<div class="col-md-11 text-left my-2">
							<i class="fas fa-lock prefix"></i> <label for="psw"><b>Your Password</b></label>
							<input type="password" placeholder="Enter Password" name="password" id="password" required>
						</div>
						<div class="col-md-11">
							<button type="submit">Login</button>
						</div>
						<div class="col-md-11">
							<button type="button" onclick="document.getElementById('id01').style.display='none'"
								class="cancelbtn text-center btn-danger">Cancel</button>
						</div>
		
						<div class="col-md-12 text-left mb-3">
							<strong><span class="">Forgot <a href="/savePass">password?</a></span></strong>
							<br>
							<strong><span class="">Don't have an account? <a href="/register">Create account</a></span></strong>
						</div>
					</div>
				</div>
			</div>
			
		</div>

	</form>
</div>

<script>
	// Get the modal
	var price = document.getElementsByClassName("single-product-pricing")
	var modal = document.getElementById('id01');

	var conDecimal = parseDouble(price.toFixed(2));

	price.document.textContent = conDecimal;

	// When the user clicks anywhere outside of the modal, close it
	window.onclick = function (event) {
		if (event.target == modal) {
			modal.style.display = "none";
		}
	}
</script>
